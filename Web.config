<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.0" />
    </system.web>

    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IRentIt" />
                <binding name="BasicHttpBinding_IRentitService" />         
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://rentit.itu.dk/RentIt00/RentIt.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRentIt"
                contract="RentIt00.IRentIt" name="BasicHttpBinding_IRentIt" />
            <endpoint address="http://rentit.itu.dk/RentIt29WCF/RentitService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRentitService"
                contract="RentItServices.IRentitService" name="BasicHttpBinding_IRentitService" />
        </client>
    </system.serviceModel>
</configuration>
